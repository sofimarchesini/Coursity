t.js",["107"],"import { createContext, useState } from \"react\";\r\nexport const context = createContext();\r\nexport const { Provider } = context;\r\n\r\nconst CustomProvider = ({ children }) => {\r\n\r\n    const [cart, setCart] = useState([])\r\n\r\n    const addToCart = producto => {\r\n         if (cart.find(item => item.id === producto.id)) {\r\n            setCart(cart.map(item => item.id === producto.id ? {\r\n                ...item,quantity: item.quantity + producto.quantity\r\n            } : item))\r\n\r\n        } else {\r\n            setCart([...cart, producto])}\r\n    }\r\n\r\n    const removeFromCart = (prod) => {\r\n        setCart(cart.filter(item => item.id !== prod.id))\r\n        prod.quantity = 0;\r\n    }\r\n\r\n    const clearCart = () => setCart([])\r\n    const subTotal = (prod) => {return prod.price * prod.quantity }\r\n\r\n    const sumQuantity = () => {\r\n        let sum = 0\r\n        cart.forEach(item => {\r\n            sum += item.quantity\r\n        })\r\n        return sum\r\n    }\r\n\r\n    function freshTotal(){\r\n        let tempTotal=0;\r\n        cart.map(item => {\r\n             tempTotal += parseInt(item.price*item.quantity);\r\n        });\r\n        console.log(tempTotal);\r\n        return  tempTotal;\r\n    }\r\n\r\n    const context_values = {\r\n        cart: cart,\r\n        addToCart: addToCart,\r\n        removeFromCart: removeFromCart,\r\n        clearCart: clearCart,\r\n        subTotal: subTotal,\r\n        sumQuantity: sumQuantity,\r\n        freshTotal: freshTotal\r\n    }\r\n\r\n    return <Provider value={ context_values }>{ children }</Provider>\r\n}\r\n\r\n\r\nexport default CustomProvider;","C:\\Users\\sofia\\Downloads\\TiendaMarchesini-main\\src\\components\\Home\\home.js",[],"C:\\Users\\sofia\\Downloads\\TiendaMarchesini-main\\src\\components\\Cart\\Cart.js",["108","109","110","111","112"],"\r\nimport { useContext } from 'react';\r\nimport { context } from '../context/cartContext'\r\nimport { Font